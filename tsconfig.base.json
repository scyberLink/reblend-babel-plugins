{
  "compilerOptions": {
    //"composite": true,
    //"rootDir": ".",
    "target": "ESNext",
    "module": "CommonJS",
    "lib": ["ESNext"],
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": false,
    "esModuleInterop": true,
    "isolatedModules": false,
    "allowImportingTsExtensions": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "strictBindCallApply": true
  },
  "exclude": ["**/test/**"]
}
