{
  "name": "reblend-babel-plugins",
  "version": "1.0.0",
  "codename": "infant",
  "description": "ReblendJs babel plugins",
  "repository": "https://github.com/scyberLink/reblend-babel-plugins",
  "keywords": [
    "ReblendJs",
    "Reative",
    "UI",
    "Library",
    "Babel",
    "Plugins"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "changelog": "lerna-changelog",
    "publish": "tasks/publish.sh",
    "compile": "npx tsc",
    "alex": "alex .",
    "eslint": "eslint .",
    "prettier": "prettier ./packages",
    "format": "npx prettier --write ./packages"
  },
  "author": "Emmanuel Elom <scansioquielom@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.4.5",
    "alex": "^8.2.0",
    "eslint": "^8.3.0",
    "husky": "^4.3.8",
    "jest": "^27.4.3",
    "lerna": "^4.0.0",
    "lerna-changelog": "^2.2.0",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,yml,yaml,css,scss,ts,tsx,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "eslint-config-reblend-app": "^7.0.5"
  },
  "workspaces": [
    "packages/*"
  ]
}
