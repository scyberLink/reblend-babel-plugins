{
  "name": "reblend-babel-plugins",
  "version": "1.0.0",
  "codename": "infant",
  "description": "ReblendJs babel plugins",
  "repository": "https://github.com/scyberLink/reblend-babel-plugins.git",
  "keywords": [
    "ReblendJs",
    "Reative",
    "UI",
    "Library",
    "Babel",
    "Plugins"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "private": true,
  "scripts": {
    "changelog": "lerna-changelog",
    "npm": "tasks/npm.sh",
    "publish": "npm run build; tasks/publish.sh",
    "build": "lerna run build",
    "compile": "npx tsc",
    "compile:packages": "tasks/compile.sh",
    "alex": "alex .",
    "eslint": "eslint .",
    "prettier": "prettier ./packages",
    "format": "npx prettier --write ./packages"
  },
  "author": "Emmanuel Elom <scansioquielom@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "alex": "^8.2.0",
    "eslint": "^8.57.1",
    "husky": "^4.3.8",
    "jest": "^27.4.3",
    "lerna": "^4.0.0",
    "lerna-changelog": "^2.2.0",
    "lint-staged": "^12.1.2",
    "prettier": "^3.3.2",
    "typescript": "^5.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,yml,yaml,css,scss,ts,tsx,md}": [
      "prettier --write"
    ]
  },
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "yarn@4.9.2+sha512.1fc009bc09d13cfd0e19efa44cbfc2b9cf6ca61482725eb35bbc5e257e093ebf4130db6dfe15d604ff4b79efd8e1e8e99b25fa7d0a6197c9f9826358d4d65c3c"
}
